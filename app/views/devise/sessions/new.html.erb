
<div>
  <div> 
    <%= simple_form_for(resource, :as => resource_name, :url => session_path(resource_name), html: {class: "login"}) do |f| %>
    <div class="login">
        <fieldset>
          <div class="page-header">
              <h1>Sign In</h1>
          </div>
          <%= f.input :email %>
          <%= f.input :password %>



          <%= f.button :submit, "Sign in", class: "btn-warning" %>
          <%= link_to "Cancel", :back, class: "btn" %>

        </fieldset> 
    <% end %>

    <%= render "devise/shared/links" %>
    </div>
  </div>
</div>